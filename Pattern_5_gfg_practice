class Solution {

    void printTriangle(int n) {
        
        int s = n ;
        
        for (int i =1 ; i <= n ; i ++){
            
            for( int j = 1; j <= s ; j ++){
                
                System.out.print("* ");
            }
            
            s --;
            
            System.out.println();
        }

    }
}
