class Solution {

    void printTriangle(int n) {
        
        int s = 1 ;
        
        for (int i =0 ; i < n ; i ++){
            
            for( int j = 0; j < s ; j ++){
                
                System.out.print("* ");
            }
            
            s ++;
            
            System.out.println();
        }

    }
}
