class Solution {
    public String removeOuterParentheses(String s) {
        String result="";
        int cnt=0;
        for(int i=0;i<s.length();i++){
            if(s.charAt(i)=='('){
                if(cnt>0){
                    result+=(s.charAt(i));
                }
                cnt++;
            }
            else{
                cnt--;
                if(cnt>0){
                    result+=(s.charAt(i));
                }
            }
        }
        return result;
    }
}
